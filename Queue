int isQueu
eEmpty()

 {

     if(front == rear)

         return 1;

     return -1;

 }

 //removes the current beginning element from the queue.

 void dequeue()

 {

     if(isQueueEmpty() == 1)

         printf("Queue is Empty.\n");

     else

     {

         printf("Dequeued element = %d\n",arr[front]);

